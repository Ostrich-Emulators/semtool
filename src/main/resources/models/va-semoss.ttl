insights:Generic-Perspective dcterms:created "2015-04-30T21:13:48.141Z"^^xsd:dateTime ;
	dcterms:creator "V-CAMP SEMOSS Tool v2015.03" ;
	olo:slot insights:Generic-Perspective-slot-1 , insights:Generic-Perspective-slot-2 ;
	a vas:Perspective ;
	rdfs:label "Generic-Perspective" ;
	dcterms:description "System Provided Generic Perspective" ;
	dcterms:modified "2015-04-30T21:13:48.141Z"^^xsd:dateTime .

insights:Generic-Perspective-slot-1 olo:index "1"^^xsd:int ;
	olo:item insights:GQ0 .

insights:Generic-Perspective-slot-2 olo:index "2"^^xsd:int ;
	olo:item insights:GQ1 .

insights:GQ0 dcterms:created "2015-04-30T21:13:48.141Z"^^xsd:dateTime ;
	dcterms:creator "V-CAMP SEMOSS Tool v2015.03" ;
	vas:isLegacy "true"^^xsd:boolean ;
	spin:body insights:GQ0-SELECT ;
	ui:dataView vas:gov.va.semoss.ui.components.playsheets.MetamodelGraphPlaySheet ;
	a spin:MagicProperty ;
	rdfs:label "View the Database Metamodel" ;
	vas:rendererClass "" ;
	rdfs:subClassOf vas:InsightProperties ;
	dcterms:description "System provided model explorer query." ;
	dcterms:modified "2015-04-30T21:13:48.141Z"^^xsd:dateTime .

insights:GQ0-SELECT sp:text "SELECT DISTINCT ?source ?relation ?target WHERE { ?relation a owl:ObjectProperty . ?s ?relation ?o . ?s a ?source . ?o a ?target . FILTER( ?source != <http://semoss.org/ontologies/Concept> && ?source != owl:Class && ?source != rdfs:Resource ) FILTER( ?target != <http://semoss.org/ontologies/Concept> && ?target != owl:Class && ?target != rdfs:Resource ) }" ;
	a sp:Select .

insights:GQ1 dcterms:created "2015-04-30T21:13:48.141Z"^^xsd:dateTime ;
	dcterms:creator "V-CAMP SEMOSS Tool v2015.03" ;
	vas:isLegacy "true"^^xsd:boolean ;
	spin:body insights:GQ1-CONSTRUCT ;
	spin:constraint insights:GQ1-Concept , insights:GQ1-Instance ;
	ui:dataView vas:gov.va.semoss.ui.components.playsheets.GraphPlaySheet ;
	a spin:MagicProperty ;
	rdfs:label "Explore an instance of a selected node type" ;
	vas:rendererClass "" ;
	rdfs:subClassOf vas:InsightProperties ;
	dcterms:description "System provided instance explorer query." ;
	dcterms:modified "2015-04-30T21:13:48.141Z"^^xsd:dateTime .

insights:GQ1-Concept sp:query insights:GQ1-Concept-Query ;
	spl:predicate arg:Concept ;
	spl:valueType semoss:Concept ;
	a spl:Argument ;
	rdfs:label "Concept" .

arg:Concept rdfs:label "Concept" .

insights:GQ1-Concept-Query sp:text "SELECT ?entity WHERE { {?entity rdfs:subClassOf <http://semoss.org/ontologies/Concept> ;} }" ;
	a sp:Select .

insights:GQ1-CONSTRUCT sp:text "CONSTRUCT {?instance ?instance ?instance} WHERE { BIND(<@Concept-http://semoss.org/ontologies/Concept@> AS ?entity) BIND(<@Instance-http://semoss.org/ontologies/Concept/Instance@> AS ?instance) {?instance a ?entity ;} }" ;
	a sp:Construct .

insights:GQ1-Instance sp:query insights:GQ1-Instance-Query ;
	spl:predicate arg:Instance ;
	spl:valueType semoss:Concept ;
	a spl:Argument ;
	rdfs:label "Instance" .

arg:Instance rdfs:label "Instance" .

insights:GQ1-Instance-Query sp:text "SELECT ?Instance WHERE { {?Instance a <@Concept@> ;} }" ;
	a sp:Select .

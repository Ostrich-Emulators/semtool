RP1_LAYOUT prerna.ui.components.playsheets.GridPlaySheet
RP1_QUERY	SELECT DISTINCT ?Requirement ?ApplicationModule WHERE {BIND(<@Requirement-http://semoss.org/ontologies/Requirement@> AS ?Requirement){?Fulfills rdfs:subPropertyOf <http://semoss.org/ontologies/Relation/Fulfills>;}{?ApplicationModule a <http://semoss.org/ontologies/ApplicationModule>;}{?ApplicationModule ?Fulfills ?Requirement}}

RP2_LAYOUT prerna.ui.components.playsheets.GridPlaySheet
RP2_QUERY	SELECT DISTINCT ?Requirement ?Activity WHERE{BIND(<@Requirement-http://semoss.org/ontologies/Requirement@> AS ?Requirement){?Fulfills rdfs:subPropertyOf <http://semoss.org/ontologies/Relation/Fulfills>;}{?DataObject a <http://semoss.org/ontologies/DataObject>;}{?Activity a <http://semoss.org/ontologies/Activity>;}{?Needs rdfs:subPropertyOf <http://semoss.org/ontologies/Relation/Needs>;}{?DataObject a <http://semoss.org/ontologies/DataObject>;}{?Requirement ?Fulfills ?DataObject}{?Activity ?Needs ?DataObject}}

RP3_LAYOUT prerna.ui.components.playsheets.GridPlaySheet
RP3_QUERY	SELECT DISTINCT ?Activity ?Requirement WHERE{{?Requirement a <http://semoss.org/ontologies/Requirement>;}{?Fulfills rdfs:subPropertyOf <http://semoss.org/ontologies/Relation/Fulfills>;}{?DataObject a <http://semoss.org/ontologies/DataObject>;}BIND(<@Activity-http://semoss.org/ontologies/Activity@> AS ?Activity){?Needs rdfs:subPropertyOf <http://semoss.org/ontologies/Relation/Needs>;}{?DataObject a <http://semoss.org/ontologies/DataObject>;}{?Requirement ?Fulfills ?DataObject}{?Activity ?Needs ?DataObject}}

RP4_LAYOUT	prerna.ui.components.playsheets.GridPlaySheet
RP4_QUERY	SELECT DISTINCT ?Requirement ?DataObject ?ApplicationModule WHERE {BIND(<@Requirement-http://semoss.org/ontologies/Requirement@> AS ?Requirement){?Provides rdfs:subPropertyOf <http://semoss.org/ontologies/Relation/Provides>;}{?DataObject a <http://semoss.org/ontologies/DataObject> ;}{?Fulfills rdfs:subPropertyOf <http://semoss.org/ontologies/Relation/Fulfills>;}{?ApplicationModule a <http://semoss.org/ontologies/ApplicationModule>;}{?Requirement ?Fulfills ?DataObject}{?ApplicationModule ?Provides ?DataObject}}

RP5_LAYOUT	prerna.ui.components.playsheets.GridPlaySheet
RP5_QUERY	SELECT DISTINCT ?Requirement ?BusinessLogicUnit ?ApplicationModule WHERE {BIND(<@Requirement-http://semoss.org/ontologies/Requirement@> AS ?Requirement){?Provides rdfs:subPropertyOf <http://semoss.org/ontologies/Relation/Provides>;}{?BusinessLogicUnit a <http://semoss.org/ontologies/BusinessLogicUnit> ;}{?Fulfills rdfs:subPropertyOf <http://semoss.org/ontologies/Relation/Fulfills>;}{?ApplicationModule a <http://semoss.org/ontologies/ApplicationModule>;}{?Requirement ?Fulfills ?BusinessLogicUnit}{?ApplicationModule ?Provides ?BusinessLogicUnit}}

RP6_LAYOUT	prerna.ui.components.playsheets.HeatMapPlaySheet
RP6_QUERY	SELECT DISTINCT ?ApplicationModule ?Requirement ((COUNT(DISTINCT(?DataObject)) / COUNT(DISTINCT(?DataObject1)))*100*IF(sample(?rCMR)='C', IF(sample(?aCMR)='C', 1, 0), IF(sample(?rCMR)='M', IF(sample(?aCMR)='C', 1, IF(sample(?aCMR)='M',1,0)), IF(sample(?rCMR)='R', IF(sample(?aCMR)='C', 1, IF(sample(?aCMR)='M',1,IF(sample(?aCMR)='R',1,0))), 0) )) AS ?ctd) WHERE {{?Requirement a <http://semoss.org/ontologies/Requirement>;}{?DataObject a <http://semoss.org/ontologies/DataObject>;}{?Fulfills rdfs:subPropertyOf <http://semoss.org/ontologies/Relation/Fulfills>;}{?Requirement ?Fulfills ?DataObject} {?ApplicationModule a <http://semoss.org/ontologies/ApplicationModule>;}{?Provides rdfs:subPropertyOf <http://semoss.org/ontologies/Relation/Provides>;}{?Provides <http://semoss.org/ontologies/CMR> ?aCMR;}{?ApplicationModule ?Provides ?DataObject}{?DataObject1 a <http://semoss.org/ontologies/DataObject>;}{?Fulfills1 rdfs:subPropertyOf <http://semoss.org/ontologies/Relation/Fulfills>;}{?Fulfills1 <http://semoss.org/ontologies/CMR> ?rCMR;}{?Requirement ?Fulfills1 ?DataObject1} } GROUP BY ?ApplicationModule ?Requirement

RP7_LAYOUT	prerna.ui.components.playsheets.HeatMapPlaySheet
RP7_QUERY	SELECT DISTINCT ?ApplicationModule ?Requirement ((COUNT(DISTINCT(?BusinessLogicUnit)) / COUNT(DISTINCT(?BusinessLogicUnit1)))*100 AS ?ctd) WHERE {{?Requirement a <http://semoss.org/ontologies/Requirement>;}{?BusinessLogicUnit a <http://semoss.org/ontologies/BusinessLogicUnit>;}{?Fulfills rdfs:subPropertyOf <http://semoss.org/ontologies/Relation/Fulfills>;}{?Requirement ?Fulfills ?BusinessLogicUnit}{?ApplicationModule a <http://semoss.org/ontologies/ApplicationModule>;}{?Provides rdfs:subPropertyOf <http://semoss.org/ontologies/Relation/Provides>;}{?ApplicationModule ?Provides ?BusinessLogicUnit}{?BusinessLogicUnit1 a <http://semoss.org/ontologies/BusinessLogicUnit>;}{?Fulfills1 rdfs:subPropertyOf <http://semoss.org/ontologies/Relation/Fulfills>;}{?Requirement ?Fulfills1 ?BusinessLogicUnit1}} GROUP BY ?ApplicationModule ?Requirement

RP8_LAYOUT	prerna.ui.components.playsheets.HeatMapPlaySheet
RP8_QUERY	SELECT DISTINCT ?ApplicationModule ?Requirement (COALESCE(((COUNT(DISTINCT(?DataObject))*IF(sample(?rCMR)='C', IF(sample(?aCMR)='C', 1, 0), IF(sample(?rCMR)='M', IF(sample(?aCMR)='C', 1, IF(sample(?aCMR)='M',1,0)), IF(sample(?rCMR)='R', IF(sample(?aCMR)='C', 1, IF(sample(?aCMR)='M',1,IF(sample(?aCMR)='R',1,0))), 0) )) + COUNT(DISTINCT(?BusinessLogicUnit)))/(COUNT(DISTINCT(?DataObject1)) + COUNT(DISTINCT(?BusinessLogicUnit1))))*100,((COUNT(DISTINCT(?BusinessLogicUnit))/((COUNT(DISTINCT(?DataObject1)) + COUNT(DISTINCT(?BusinessLogicUnit1)))))*100)) AS ?Ratio)WHERE {{?Requirement a <http://semoss.org/ontologies/Requirement>;}{{?DataObject a <http://semoss.org/ontologies/DataObject>;}{?Fulfills rdfs:subPropertyOf <http://semoss.org/ontologies/Relation/Fulfills>;}{?Requirement ?Fulfills ?DataObject}{?ApplicationModule a <http://semoss.org/ontologies/ApplicationModule>;}{?Provides rdfs:subPropertyOf <http://semoss.org/ontologies/Relation/Provides>;}{?Provides <http://semoss.org/ontologies/CMR> ?aCMR;}{?ApplicationModule ?Provides ?DataObject}} UNION{{?BusinessLogicUnit a <http://semoss.org/ontologies/BusinessLogicUnit>;}{?FulfillsB rdfs:subPropertyOf <http://semoss.org/ontologies/Relation/Fulfills>;}{?Requirement ?FulfillsB ?BusinessLogicUnit}{?ApplicationModule a <http://semoss.org/ontologies/ApplicationModule>;}{?Provides1 rdfs:subPropertyOf <http://semoss.org/ontologies/Relation/Provides>;}{?ApplicationModule ?Provides1 ?BusinessLogicUnit}}{{?DataObject1 a <http://semoss.org/ontologies/DataObject>;}{?Fulfills1 rdfs:subPropertyOf <http://semoss.org/ontologies/Relation/Fulfills>;}{?Fulfills1 <http://semoss.org/ontologies/CMR> ?rCMR;}{?Requirement ?Fulfills1 ?DataObject1} } UNION { {?BusinessLogicUnit1 a <http://semoss.org/ontologies/BusinessLogicUnit>;}{?Fulfills1B rdfs:subPropertyOf <http://semoss.org/ontologies/Relation/Fulfills>;}{?Requirement ?Fulfills1B ?BusinessLogicUnit1} } } GROUP BY ?ApplicationModule ?Requirement

RP9_LAYOUT	prerna.ui.components.playsheets.HeatMapPlaySheet
RP9_QUERY	SELECT DISTINCT ?ApplicationModule ?BusinessRule ((COUNT(DISTINCT(?DataObject)) / COUNT(DISTINCT(?DataObject1)))*100*IF(sample(?rCMR)='C', IF(sample(?aCMR)='C', 1, 0), IF(sample(?rCMR)='M', IF(sample(?aCMR)='C', 1, IF(sample(?aCMR)='M',1,0)), IF(sample(?rCMR)='R', IF(sample(?aCMR)='C', 1, IF(sample(?aCMR)='M',1,IF(sample(?aCMR)='R',1,0))), 0) )) AS ?ctd) WHERE {{?Requirement a <http://semoss.org/ontologies/Requirement>;}{?DataObject a <http://semoss.org/ontologies/DataObject>;}{?Fulfills rdfs:subPropertyOf <http://semoss.org/ontologies/Relation/Fulfills>;}{?Requirement ?Fulfills ?DataObject} {?ApplicationModule a <http://semoss.org/ontologies/ApplicationModule>;}{?Provides rdfs:subPropertyOf <http://semoss.org/ontologies/Relation/Provides>;}{?Provides <http://semoss.org/ontologies/CMR> ?aCMR;}{?ApplicationModule ?Provides ?DataObject}{?DataObject1 a <http://semoss.org/ontologies/DataObject>;}{?Fulfills1 rdfs:subPropertyOf <http://semoss.org/ontologies/Relation/Fulfills>;}{?Fulfills1 <http://semoss.org/ontologies/CMR> ?rCMR;}{?Requirement ?Fulfills1 ?DataObject1} {?BusinessRule a <http://semoss.org/ontologies/BusinessRule>;}{?TypeOf rdfs:subPropertyOf <http://semoss.org/ontologies/Relation/TypeOf>;}{?BusinessRule ?TypeOf ?Requirement} } GROUP BY ?ApplicationModule ?BusinessRule

RP10_LAYOUT	prerna.ui.components.playsheets.HeatMapPlaySheet
RP10_QUERY	SELECT DISTINCT ?ApplicationModule ?BusinessRule ((COUNT(DISTINCT(?BusinessLogicUnit)) / COUNT(DISTINCT(?BusinessLogicUnit1)))*100 AS ?ctd) WHERE {{?Requirement a <http://semoss.org/ontologies/Requirement>;}{?BusinessLogicUnit a <http://semoss.org/ontologies/BusinessLogicUnit>;}{?Fulfills rdfs:subPropertyOf <http://semoss.org/ontologies/Relation/Fulfills>;}{?Requirement ?Fulfills ?BusinessLogicUnit} {?ApplicationModule a <http://semoss.org/ontologies/ApplicationModule>;}{?Provides rdfs:subPropertyOf <http://semoss.org/ontologies/Relation/Provides>;}{?ApplicationModule ?Provides ?BusinessLogicUnit} {?BusinessLogicUnit1 a <http://semoss.org/ontologies/BusinessLogicUnit>;}{?Fulfills1 rdfs:subPropertyOf <http://semoss.org/ontologies/Relation/Fulfills>;}{?Requirement ?Fulfills1 ?BusinessLogicUnit1}{?BusinessRule a <http://semoss.org/ontologies/BusinessRule>;}{?TypeOf rdfs:subPropertyOf <http://semoss.org/ontologies/Relation/TypeOf>;}{?BusinessRule ?TypeOf ?Requirement} } GROUP BY ?ApplicationModule ?BusinessRule

RP11_LAYOUT	prerna.ui.components.playsheets.HeatMapPlaySheet
RP11_QUERY	SELECT DISTINCT ?ApplicationModule ?BusinessRule (((COUNT(DISTINCT(?DataObject))*IF(sample(?rCMR)='C', IF(sample(?aCMR)='C', 1, 0), IF(sample(?rCMR)='M', IF(sample(?aCMR)='C', 1, IF(sample(?aCMR)='M',1,0)), IF(sample(?rCMR)='R', IF(sample(?aCMR)='C', 1, IF(sample(?aCMR)='M',1,IF(sample(?aCMR)='R',1,0))), 0) )) + (COUNT(DISTINCT(?BusinessLogicUnit))))/(COUNT(DISTINCT(?DataObject1)) + COUNT(DISTINCT(?BusinessLogicUnit1))))*100 AS ?Ratio)WHERE {{?Requirement a <http://semoss.org/ontologies/Requirement>;}{{?DataObject a <http://semoss.org/ontologies/DataObject>;}{?Fulfills rdfs:subPropertyOf <http://semoss.org/ontologies/Relation/Fulfills>;}{?Requirement ?Fulfills ?DataObject}{?ApplicationModule a <http://semoss.org/ontologies/ApplicationModule>;}{?Provides rdfs:subPropertyOf <http://semoss.org/ontologies/Relation/Provides>;}{?Provides <http://semoss.org/ontologies/CMR> ?aCMR;}{?ApplicationModule ?Provides ?DataObject}{?TypeOf rdfs:subPropertyOf <http://semoss.org/ontologies/Relation/TypeOf>;}{?BusinessRule a <http://semoss.org/ontologies/BusinessRule>;}{?BusinessRule ?TypeOf ?Requirement}} UNION{{?BusinessLogicUnit a <http://semoss.org/ontologies/BusinessLogicUnit>;}{?FulfillsB rdfs:subPropertyOf <http://semoss.org/ontologies/Relation/Fulfills>;}{?Requirement ?FulfillsB ?BusinessLogicUnit}{?ApplicationModule a <http://semoss.org/ontologies/ApplicationModule>;}{?Provides1 rdfs:subPropertyOf <http://semoss.org/ontologies/Relation/Provides>;}{?ApplicationModule ?Provides1 ?BusinessLogicUnit}}{{?DataObject1 a <http://semoss.org/ontologies/DataObject>;}{?Fulfills1 rdfs:subPropertyOf <http://semoss.org/ontologies/Relation/Fulfills>;}{?Fulfills1 <http://semoss.org/ontologies/CMR> ?rCMR;}{?Requirement ?Fulfills1 ?DataObject1} } UNION { {?BusinessLogicUnit1 a <http://semoss.org/ontologies/BusinessLogicUnit>;}{?Fulfills1B rdfs:subPropertyOf <http://semoss.org/ontologies/Relation/Fulfills>;}{?Requirement ?Fulfills1B ?BusinessLogicUnit1} } } GROUP BY ?ApplicationModule ?BusinessRule

RP12_LAYOUT	prerna.ui.components.playsheets.GridPlaySheet
RP12_QUERY	SELECT DISTINCT ?ApplicationModule ?BusinessRule WHERE { BIND(<@ApplicationModule-http://semoss.org/ontologies/ApplicationModule@> AS ?ApplicationModule) . ?ApplicationModule a <http://semoss.org/ontologies/ApplicationModule> . ?Implements rdfs:subPropertyOf <http://semoss.org/ontologies/Relation/Implements> . ?BusinessRule a <http://semoss.org/ontologies/BusinessRule> . ?ApplicationModule ?Implements ?BusinessRule .}

RP13_LAYOUT	prerna.ui.components.playsheets.GridPlaySheet
RP13_QUERY SELECT DISTINCT ?Activity ?Requirement WHERE { BIND(<@Activity-http://semoss.org/ontologies/Activity@> AS ?Activity) . ?Fulfills rdfs:subPropertyOf <http://semoss.org/ontologies/Relation/Fulfills> . ?Requirement a <http://semoss.org/ontologies/Requirement> . ?Activity a <http://semoss.org/ontologies/Activity> . ?Supports rdfs:subPropertyOf <http://semoss.org/ontologies/Relation/Supports> . ?ApplicationModule ?Fulfills ?Requirement . ?ApplicationModule ?Supports ?Activity .}

RP14_LAYOUT	prerna.ui.components.playsheets.HeatMapPlaySheet
RP14_QUERY	SELECT DISTINCT ?ApplicationModule ?Requirement (COALESCE(((COUNT(DISTINCT(?DataObject))*IF(sample(?rCMR)='C', IF(sample(?aCMR)='C', 1, 0), IF(sample(?rCMR)='M', IF(sample(?aCMR)='C', 1, IF(sample(?aCMR)='M',1,0)), IF(sample(?rCMR)='R', IF(sample(?aCMR)='C', 1, IF(sample(?aCMR)='M',1,IF(sample(?aCMR)='R',1,0))), 0) )) + COUNT(DISTINCT(?BusinessLogicUnit)))/(COUNT(DISTINCT(?DataObject1)) + COUNT(DISTINCT(?BusinessLogicUnit1))))*100,((COUNT(DISTINCT(?BusinessLogicUnit))/((COUNT(DISTINCT(?DataObject1)) + COUNT(DISTINCT(?BusinessLogicUnit1)))))*100)) AS ?Ratio) WHERE { {?Requirement a <http://semoss.org/ontologies/Requirement>;}{{?DataObject a <http://semoss.org/ontologies/DataObject>;}{?Fulfills rdfs:subPropertyOf <http://semoss.org/ontologies/Relation/Fulfills>;}{?Requirement ?Fulfills ?DataObject}{?ApplicationModule a <http://semoss.org/ontologies/ApplicationModule>;}{?Provides rdfs:subPropertyOf <http://semoss.org/ontologies/Relation/Provides>;}{?Provides <http://semoss.org/ontologies/CMR> ?aCMR;}{?ApplicationModule ?Provides ?DataObject}} UNION{{?BusinessLogicUnit a <http://semoss.org/ontologies/BusinessLogicUnit>;}{?FulfillsB rdfs:subPropertyOf <http://semoss.org/ontologies/Relation/Fulfills>;}{?Requirement ?FulfillsB ?BusinessLogicUnit}{?ApplicationModule a <http://semoss.org/ontologies/ApplicationModule>;}{?Provides1 rdfs:subPropertyOf <http://semoss.org/ontologies/Relation/Provides>;}{?ApplicationModule ?Provides1 ?BusinessLogicUnit}}{{?DataObject1 a <http://semoss.org/ontologies/DataObject>;}{?Fulfills1 rdfs:subPropertyOf <http://semoss.org/ontologies/Relation/Fulfills>;}{?Fulfills1 <http://semoss.org/ontologies/CMR> ?rCMR;}{?Requirement ?Fulfills1 ?DataObject1} } UNION { {?BusinessLogicUnit1 a <http://semoss.org/ontologies/BusinessLogicUnit>;}{?Fulfills1B rdfs:subPropertyOf <http://semoss.org/ontologies/Relation/Fulfills>;}{?Requirement ?Fulfills1B ?BusinessLogicUnit1} } } GROUP BY ?ApplicationModule ?Requirement

RP15_LAYOUT prerna.ui.components.playsheets.HeatMapPlaySheet
RP15_QUERY SELECT DISTINCT ?BusinessRule ?Requirement (1 AS ?Ct) WHERE { BIND(<@ApplicationModule-http://semoss.org/ontologies/ApplicationModule@> AS ?ApplicationModule) . ?ApplicationModule a <http://semoss.org/ontologies/ApplicationModule> . ?Fulfills rdfs:subPropertyOf <http://semoss.org/ontologies/Relation/Fulfills> . ?Requirement a <http://semoss.org/ontologies/Requirement> . ?ApplicationModule ?Fulfills ?Requirement . OPTIONAL {?Implements rdfs:subPropertyOf <http://semoss.org/ontologies/Relation/Implements> . ?BusinessRule a <http://semoss.org/ontologies/BusinessRule> . ?ApplicationModule ?Implements ?BusinessRule . ?Fulfills1 rdfs:subPropertyOf <http://semoss.org/ontologies/Relation/Fulfills> . ?Requirement ?Fulfills1 ?BusinessRule .} }


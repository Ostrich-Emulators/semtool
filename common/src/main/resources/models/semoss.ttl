semoss:ApplicationModule
  rdf:type owl:Class ;
  vac:description "An application module is a distinct module within an application that has a specific purpose. The existing set of DoD-VA Application "^^xsd:string ;
  rdfs:label "Application Module"^^xsd:string ;
  rdfs:subClassOf semoss:Concept ;
.
semoss:BFF25Activity
  rdf:type owl:Class ;
  rdfs:label "BFF25 Activity"^^xsd:string ;
  rdfs:subClassOf semoss:Concept ;
.
semoss:BusinessLogicUnit
  rdf:type owl:Class ;
  vac:description "A Business Logic Unit is a functional, automated algorithm that the enterprise uses to transform inputs into outputs. BLUs are needed to perform business activities. "^^xsd:string ;
  rdfs:label "Business Logic Unit"^^xsd:string ;
  rdfs:subClassOf semoss:Concept ;
.
semoss:BusinessProcess
  rdf:type owl:Class ;
  vac:description "A Business Process is a collection of related, structured activities or tasks that produce a specific service or product (serve a particular goal) for a particular customer or customers. One or more business processes support each capability. "^^xsd:string ;
  rdfs:label "Business Process"^^xsd:string ;
  rdfs:subClassOf semoss:Concept ;
.
semoss:BusinessRule
  rdf:type owl:Class ;
  vac:description "A business rule is a high-level functionality condition that the system must support in order to complete the business of the organization. Business rules describe the operations, definitions and constraints that apply to an organization."^^xsd:string ;
  rdfs:label "Business Rule"^^xsd:string ;
  rdfs:subClassOf semoss:Concept ;
.
semoss:Capability
  rdf:type owl:Class ;
  vac:description "A Capability is a high-level grouping of different types of missions within the organization. "^^xsd:string ;
  rdfs:label "Capability"^^xsd:string ;
  rdfs:subClassOf semoss:Concept ;
  rdfs:subClassOf [
      rdf:type owl:Restriction ;
      owl:allValuesFrom semoss:ApplicationModule ;
      owl:onProperty semoss:consists ;
    ] ;
.
semoss:DBIANumber
  rdf:type owl:Class ;
  rdfs:label "DBIA Number"^^xsd:string ;
  rdfs:subClassOf semoss:Concept ;
.
semoss:DataElement
  rdf:type owl:Class ;
  vac:description "A data element is a unit of decomposition that identifies specific information or fields within a Data Object. An example might be a social security number or a first name. "^^xsd:string ;
  rdfs:label "Data Element"^^xsd:string ;
  rdfs:subClassOf semoss:Concept ;
.
semoss:DataObject
  rdf:type owl:Class ;
  vac:description "A Data Object is a grouping of data elements into a logical unit of similar types of data. "^^xsd:string ;
  rdfs:label "Data Object"^^xsd:string ;
  rdfs:subClassOf semoss:Concept ;
.
semoss:InterfaceControlDocument
  rdf:type owl:Class ;
  vac:description "An Interface Control Document describes how two applications communicate with one another. It specifies what data is being exchanged, how frequently, and in what format."^^xsd:string ;
  rdfs:label "Interface Control Document"^^xsd:string ;
  rdfs:subClassOf semoss:Concept ;
.
semoss:InterfaceElement
  rdf:type owl:Class ;
  rdfs:label "Interface Element"^^xsd:string ;
  rdfs:subClassOf semoss:Concept ;
  rdfs:subClassOf [
      rdf:type owl:Restriction ;
      owl:maxCardinality "1"^^xsd:nonNegativeInteger ;
      owl:onProperty semoss:dbiaNumber ;
    ] ;
.
